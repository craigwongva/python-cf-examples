#install python dependencies
pip install dataspyre

python sine_wave.py 8080 --host=0.0.0.0

#install and activate nginx
# Reference: git clone https://github.com/craigwongva/install-reverse-proxy
sudo yum -y install epel-release
sudo yum -y install nginx httpd-tools
sudo htpasswd -cb /etc/nginx/htpasswd.users craig unused2
sudo cp sinewave.conf /etc/nginx/conf.d
#sudo service nginx start

